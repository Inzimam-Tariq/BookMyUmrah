<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.inzimamtariq.bookmyumrah">

    <application
        android:allowBackup="true"
        android:icon="@drawable/book1"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MainActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".start"
            android:label="@string/title_activity_start">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActionBar" />
        <activity
            android:name=".MainActivityWithNavigationDrawer"
            android:label="@string/title_activity_main_activity_with_navigation_drawer" />
        <activity
            android:name=".MainPage"
            android:label="@string/title_activity_main_page" />
        <activity
            android:name=".SignUp"
            android:label="@string/title_activity_sign_up"
            android:theme="@style/MyMaterialTheme" />
        <activity
            android:name=".HajjRegistration"
            android:label="@string/title_activity_hajj_registeration"
            android:theme="@style/MyMaterialTheme" />
        <activity
            android:name=".Search"
            android:label="@string/title_activity_search"
            android:theme="@style/MyMaterialTheme"></activity>
    </application>

</manifest>
